{
    "name": "sajol010/cakephp-dompdf",
    "description": "Dompdf plugin for CakePHP 5/6 maintained by sajol010",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "cakephp5",
        "cakephp6",
        "pdf",
        "dompdf",
        "html",
        "css"
    ],
    "homepage": "https://github.com/sajol010/cakephp-dompdf",
    "license": "MIT",
    "authors": [
        {
            "name": "Sajol010",
            "homepage": "https://github.com/sajol010",
            "role": "Maintainer"
        },
        {
            "name": "Flavien Beninca",
            "role": "Original author"
        }
    ],
    "require": {
        "php": "^8.1",
        "cakephp/cakephp": "^5.0 || ^6.0",
        "dompdf/dompdf": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Dompdf\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dompdf\\Test\\": "tests"
        }
    },
    "support": {
        "issues": "https://github.com/sajol010/cakephp-dompdf/issues",
        "source": "https://github.com/sajol010/cakephp-dompdf"
    }
}
